// a. Use a for loop to print numbers from 1 to the user's number.
const userNumberLimit = parseInt(prompt("Please enter a number for the loop limit: "));

for (let i = 1; i <= userNumberLimit; i++) {
  console.log(i);
}

// b. Use a while loop to count down from 5.
let count = 5;
while (count >= 1) {
  console.log(count);
  count--;
}
